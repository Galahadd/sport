@model IEnumerable<Sport.Domain.Entities.NutritionList>

<p>
    <a asp-action="Create2">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalCaloriValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EnumNutritionType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EnumNutritionKind)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalCaloriValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EnumNutritionType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EnumNutritionKind)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
